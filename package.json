{
  "name": "live-coding-classroom",
  "version": "1.0.0",
  "description": "A real-time collaborative coding platform for teachers and students",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd Server && npm start",
    "client": "cd client && npm run dev",
    "install-all": "npm install && cd Server && npm install && cd ../client && npm install"
  },
  "keywords": [
    "education",
    "coding",
    "real-time",
    "collaboration"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "dependencies": {
    "ioredis": "^5.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
